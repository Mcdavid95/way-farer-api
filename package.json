{
  "name": "way-farer-api",
  "version": "1.0.0",
  "engines": {
    "node": "8.10.0"
  },
  "description": "WayFarer is a public bus transportation booking server.",
  "main": "index.js",
  "scripts": {
    "build": "babel ./ -d build --only src --ignore node_modules --source-maps inline",
    "dev-start": "babel-watch src/index.js",
    "prebuild": "rm -rf build/*",
    "prestart": "npm run build",
    "start": "node build/src/index.js",
    "test": "nyc --reporter=text --reporter=html --reporter=lcov mocha --timeout 35000 --require @babel/register ./tests/** --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mcdavid95/way-farer-api.git"
  },
  "keywords": [
    "trip",
    "ride",
    "destination",
    "location"
  ],
  "author": "Mcdavid95",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mcdavid95/way-farer-api/issues"
  },
  "homepage": "https://github.com/Mcdavid95/way-farer-api#readme",
  "devDependencies": {
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.18.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "babel-watch": "^7.0.0",
    "bunyan": "^1.8.12",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1"
  }
}
